  {
  "compilerOptions": {
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "sourceMap": true,
    "allowJs": true,
    "outDir": ".build",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es2017"
    ],
    "rootDir": "./",
    "target": "es6",
    "noImplicitAny": false,
    "removeComments": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  },
  "exclude": [
    "node_modules",
    ".build",
    ".serverless",
  ]
}